{
  "name": "terra-draw",
  "version": "0.0.1",
  "description": "Frictionless map drawing across mapping provider",
  "scripts": {
    "build": "microbundle",
    "watch": "microbundle --watch --format modern",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:nocheck": "jest --config swc.jest.config.json",
    "test:nocheck:coverage": "jest --config swc.jest.config.json --coverage"
  },
  "type": "module",
  "source": "src/terra-draw.ts",
  "exports": {
    "require": "./dist/terra-draw.cjs",
    "default": "./dist/terra-draw.modern.js"
  },
  "main": "./dist/terra-draw.cjs",
  "module": "./dist/terra-draw.module.js",
  "unpkg": "./dist/terra-draw.umd.js",
  "author": "James Milner",
  "license": "MIT",
  "repository": "JamesLMilner/terra-draw",
  "devDependencies": {
    "@swc/jest": "^0.2.22",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@types/geojson": "^7946.0.8",
    "@types/google.maps": "^3.49.2",
    "@types/jest": "^28.1.5",
    "@types/leaflet": "^1.7.11",
    "@types/mapbox-gl": "^2.7.3",
    "@types/rbush": "^3.0.0",
    "jest": "^28.1.2",
    "leaflet": "^1.8.0",
    "mapbox-gl": "^2.9.1",
    "microbundle": "^0.15.0",
    "serve": "^13.0.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4"
  }
}
